<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.zhangpeng.greenhouse">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="app.zhangpeng.AIHouse.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="app.zhangpeng.AIHouse.House"
            android:label="@string/title_activity_house"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="app.zhangpeng.AIHouse.Note_Activity"
            android:label="@string/title_activity_note_"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>